PRAGMA database_list;
SELECT name FROM "main".sqlite_schema  WHERE type IN ('table','view')   AND name NOT LIKE 'sqlite_%'   AND name LIKE ?1 ORDER BY 1;
SELECT name FROM pragma_database_list;
-- PRAGMA database_list;
SELECT sql FROM(SELECT shell_add_schema(sql,NULL,name) AS sql, type, tbl_name, name, rowid,1 AS snum, 'main' AS sname FROM main.sqlite_schema UNION ALL SELECT shell_module_schema(name), 'table', name, name, name, 9e+99, 'main' FROM pragma_module_list) WHERE lower(tbl_name) LIKE 'data' ESCAPE '\'  AND sql IS NOT NULL ORDER BY snum, rowid;
